# ============================================
# AgenticOS - Environment Configuration
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!
# ============================================

# === CORE ===
DATABASE_URL=postgresql://user:password@localhost:5432/agentic_os
REDIS_URL=redis://localhost:6379
LOG_LEVEL=INFO

# === SUPABASE ===
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# === SECURITY ===
# API Secret Key for internal endpoints
API_SECRET_KEY=your_api_secret_key

# Encryption key for sensitive data (session IDs, credentials)
# Generate with: python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
ENCRYPTION_KEY=your_fernet_key_here

# === INSTAGRAM DM AGENT ===
INSTAGRAM_USERNAME=your_instagram_username
INSTAGRAM_PASSWORD=your_instagram_password
INSTAGRAM_SESSION_PATH=sessions/instagram_session.json

# === INSTANTLY.AI (Email Marketing) ===
INSTANTLY_API_KEY=your_instantly_api_key

# === APIFY (Web Scraping) ===
APIFY_API_KEY=your_apify_api_key
APIFY_GOOGLE_MAPS_ACTOR=compass/crawler-google-places
APIFY_CONTACT_SCRAPER_ACTOR=voyager/contact-scraper

# === GOOGLE SERVICES ===
GOOGLE_CLIENT_ID=your_client_id
GOOGLE_CLIENT_SECRET=your_client_secret
GOOGLE_API_KEY=your_api_key
GOOGLE_PROJECT_ID=your_project_id
GOOGLE_REDIRECT_URI=http://localhost:8080/oauth2callback

# Google Sheets
GOOGLE_SHEETS_SPREADSHEET_ID=your_spreadsheet_id
GOOGLE_SERVICE_ACCOUNT_FILE=service_account.json

# Google Maps
GOOGLE_MAPS_API_KEY=your_maps_api_key

# Google Analytics
GOOGLE_ANALYTICS_TRACKING_ID=UA-XXXXXXXX-X

# Google Drive
GOOGLE_DRIVE_API_KEY=your_drive_api_key

# === KLAP (Video Processing) ===
KLAP_API_KEY=your_klap_api_key
KLAP_WEBHOOK_URL=https://your-webhook-url.com/klap

# === GEMINI (AI Content) ===
GEMINI_API_KEY=your_gemini_api_key

# === LINKEDIN DM AGENT ===
LINKEDIN_EMAIL=your_linkedin_email
LINKEDIN_PASSWORD=your_linkedin_password
LINKEDIN_SESSION_PATH=sessions/linkedin_session.json

# Rate Limits (conservative defaults)
LINKEDIN_CONN_PER_DAY=20
LINKEDIN_CONN_PER_WEEK=100
LINKEDIN_MSG_PER_DAY=50
LINKEDIN_CONN_DELAY_MIN=45
LINKEDIN_CONN_DELAY_MAX=90
LINKEDIN_MSG_DELAY_MIN=60
LINKEDIN_MSG_DELAY_MAX=120

# === PHANTOMBUSTER (LinkedIn Scraping) ===
PHANTOMBUSTER_API_KEY=your_phantombuster_api_key
PHANTOMBUSTER_AGENT_ID=your_agent_id

# === AYRSHARE (Social Media) ===
AYRSHARE_API_KEY=your_ayrshare_api_key
AYRSHARE_BASE_URL=https://app.ayrshare.com/api
AYRSHARE_PROFILE_KEY=your_profile_key

# === NOTIFICATIONS ===
NOTIFICATION_WEBHOOK_URL=https://your-webhook-url.com/notify
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/xxx/yyy/zzz

# === MODAL (Serverless) ===
MODAL_TOKEN_ID=your_modal_token_id
MODAL_TOKEN_SECRET=your_modal_token_secret
